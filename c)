using System;

namespace PATrilhasAtiv04

{
	class Program
	{
		public static void Main(string[] args)
		{
			
			
			Console.WriteLine("Você possui diploma?");
			 string diploma = (Console.ReadLine());
			
			
			Console.WriteLine("Você tem mais de 21 anos?");
			string idade = (Console.ReadLine());
			
			if (diploma == "sim" || idade == "sim")
			{
				Console.WriteLine("Parabéns, você está qualificado para a vaga!");
			}
			
			else
			{
				Console.WriteLine("Você não não é qualificado para a vaga!");
			
			}
			
	
				
			
			Console.Write("Press any key to continue . . . ");
			Console.ReadKey(true);
		}
	}
}
