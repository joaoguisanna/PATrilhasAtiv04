using System;

namespace PATrilhasAtiv04

{
	class Program
	{
		public static void Main(string[] args)
		{
			
			
			Console.WriteLine("qual seu nome");
			string nome = Convert.ToString (Console.ReadLine().ToLower());
			 
			
			
			
			
			if  ((nome == "willson") || (nome == "glória"))
			{
				Console.WriteLine("Olá, seja bem vindo de volta!");

			}
			else
			{
				Console.WriteLine("desconhecido");

			}
			Console.Write("Press any key to continue . . . ");
			Console.ReadKey(true);
		}
	}
}
